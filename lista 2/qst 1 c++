#include <iostream>
#include <iomanip>  // para setprecision
using namespace std;

int main() {
    double nota1, nota2, media;

    cout << "Digite a primeira nota: ";
    cin >> nota1;

    cout << "Digite a segunda nota: ";
    cin >> nota2;

    media = (nota1 + nota2) / 2.0;

    cout << fixed << setprecision(2);
    cout << "NOTA FINAL = " << media << endl;

    if (media < 60.0) {
        cout << "REPROVADO" << endl;
    }

    return 0;
}
